<launch>
  <node pkg="choreonoid_ros" name="choreonoid" type="choreonoid"
        args="$(find amphibious_choreonoid)/projects/tank_sim.cnoid" />
  <node name="image_view" pkg="image_view" type="image_view" >
    <remap from="image" to="/amphibious_tank/camera/image" />
  </node>
  <node name="calc_motions" pkg="amphibious_choreonoid_motions" type="calc_motions" output="screen">
      <remap from="/image_raw" to="/amphibious_tank/camera/image" />
  </node>
</launch>
